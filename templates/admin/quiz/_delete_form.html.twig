<form method="post" action="{{ path('admin_quiz_delete', {'id': quiz.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');" class="w-fit mx-auto mt-4">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ quiz.id) }}">
	<button class="btn-delete">Delete</button>
</form>
