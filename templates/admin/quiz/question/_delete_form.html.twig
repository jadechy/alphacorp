<form method="post" action="{{ path('admin_question_delete', {'id': question.id}) }}" onsubmit="return confirm('Voulez-vous supprimé cette question?');" class="mx-auto w-fit mt-4">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ question.id) }}">
	<button class="btn-delete">Supprimé</button>
</form>
